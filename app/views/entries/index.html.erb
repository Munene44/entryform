<p id="notice"><%= notice %></p>

<h1>Entries</h1>

<table>
  <thead>
    <tr>
      <th>First-name</th>
      <th>Second-name</th>
      <th>Sir-name</th>
      <th>Alias</th>
      <th>Date-of-birth</th>
      <th>Address</th>
      <th>Height</th>
      <th>Photo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entries.each do |entry| %>
      <tr>
        <td><%= entry.firstname %></td>
        <td><%= entry.secondname %></td>
        <td><%= entry.sirname %></td>
        <td><%= entry.alias %></td>
        <td><%= entry.dateofbirth %></td>
        <td><%= entry.address %></td>
        <td><%= entry.height %></td>
        <td><%= entry.image %></td>
        <td><%= link_to 'Show', entry %></td>
        <td><%= link_to 'Edit', edit_entry_path(entry) %></td>
        <td><%= link_to 'Destroy', entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entry', new_entry_path %>

